#!/bin/sh

#setbg &		# Set the background
sxhkd &	# Bind keys
# Switch to US,HR or HU keyboard layout 
setxkbmap -option grp:alt_shift_toggle -layout hu,us,hr -variant &
xset r rate 300 50 &	# Speed xrate up
#xsetroot -solid '#000000'
xsetroot -cursor_name left_ptr &
unclutter &		# Remove mouse when idle
#xcompmgr &		# xcompmgr for transparency
dunst &			# dunst for notifications
mpdupdate &
#xwallpaper --no-randr --stretch $HOME/.config/wallpaper.jpg &
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
#~/.config/polybar/launch.sh &
#picom --config ~/.config/picom.conf
picom -cCGfF -o 0.38 -O 200 -I 200 -t 0 -l 0 -r 3 -D2 -m 0.88 &
